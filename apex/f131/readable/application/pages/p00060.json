     ,"Source":{
        "PL\/SQL Code":"declare\n    l_picture blob;\n    l_mimetype varchar2(50 char);\n    l_filename varchar2(200 char);\n    l_id number;\nbegin\n\n    select id, profile_picture, mimetype, filename\n    into l_id,l_picture, l_mimetype, l_filename\n    from cf_starters\n    where token = :APP_USER;\n    \n        htp.p(\u0027\u003Ca href=\"f?p=\u0026APP_ID.:60:\u0026SESSION.\"\u003E\u003Cimg id=\"profile_picture_user\" src=\"#APP_FILES#Portrait_Placeholder.png\" class=\"profile-button\";\u003E\u003C\/a\u003E\u003Ca href=\"f?p=\u0026APP_ID.:20:\u0026SESSION.\" class=\"notification\"\u003E\n        \u003Cspan class=\"fa fa-bell\" aria-hidden=\"true\"\u003E\u003C\/span\u003E  \n        \u003Cspan class=\"badge\"\u003E\u0026P60_HIDDEN_COUNT_NOTIFICATIONS.\u003C\/span\u003E\n        \u003C\/a\u003E\u0027);\n\nend;"
      }
